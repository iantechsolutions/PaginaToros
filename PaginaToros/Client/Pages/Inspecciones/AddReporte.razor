                socios = rta.List;
                var rtaa = await _solici1Servicio.Lista(0, 1000000);
                solicitudes = rtaa.List;
                var rt = await _resin1Servicio.Lista(0, 1000000);
                resultados = rt.List;
                var respuesta = await _estableServicio.Lista(0, 1000000);
                establecimientos = respuesta.List;

            }
        }
        catch(Exception ex)
